/*********** 弹出框 *************/
.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
}
/*content*/
.drawer_box {
  width: 750rpx;
  overflow: hidden;
  position: fixed;
  bottom:0;
  left: 0;
  z-index: 1001;
  background: #FAFAFA;
}
.drawer_title{
  height:50px;
  line-height:50px;
  background:#ec5958;
  color:#FFFFFF;
}
.drawer_title view{
  display: inline-block;
  line-height:50px;
  margin:0 20rpx;
}

.drawer_content {
  height: 360px;
  overflow-y: scroll; /*超出父盒子高度可滚动*/
}

.btn_ok{
  padding: 7px 0;
  font: 20px "microsoft yahei";
  text-align: center;
  background:#ec5958;
  color:#FFFFFF;
  border-radius: 5px;
  margin:5px 20rpx;
}
.conductor>view{
  display: inline-block;
  width:164.5rpx;
  margin-left:20rpx;
  height: 30px;
  line-height:30px;
  text-align: center;
  background:#dcdcdc;
  border-radius:5px;
  margin-top:5px;
  overflow: hidden;
}
.cRed{
  color:red;
  display:inline-block;
  width:10px;
}
.hot_Region_Title,.select_Area_Title{font-size:16px;margin:10px 0}
.hot_Region_Title span{background:#ec5958;}
.select_Area_Title span{background:#46b19d;}
.hot_Region_Title span,.select_Area_Title span{
  display: inline-block;
  height:16px;
  width:5px;
  margin-left:10px;
  margin-right:10px;
  margin-bottom:-2px;

}
.conductor .is_checked{
  color:#fff;
  background:#ec5958;
}
.conductor .normal{
  background: #dcdcdc;
}

.rotate_90{
  transform:rotate(90deg);
  -ms-transform:rotate(90deg); 	/* IE 9 */
  -moz-transform:rotate(90deg); 	/* Firefox */
  -webkit-transform:rotate(90deg); /* Safari 和 Chrome */
  -o-transform:rotate(90deg);
}
.rotate_180{
  transform:rotate(180deg);
  -ms-transform:rotate(180deg); 	/* IE 9 */
  -moz-transform:rotate(180deg); 	/* Firefox */
  -webkit-transform:rotate(180deg); /* Safari 和 Chrome */
  -o-transform:rotate(180deg);
}
.rotate_270{
  transform:rotate(270deg);
  -ms-transform:rotate(270deg); 	/* IE 9 */
  -moz-transform:rotate(270deg); 	/* Firefox */
  -webkit-transform:rotate(270deg); /* Safari 和 Chrome */
  -o-transform:rotate(270deg);
}
