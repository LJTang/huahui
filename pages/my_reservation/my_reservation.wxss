.denglu{
  width:650rpx;
  height:40px;
  line-height:40px;
  margin: 10px auto;
  background:#000;
  color:#fff;
}
.denglu button{
  background: #000;
  border: none;
  padding:0;
  color:#fff;
  border-radius:0;
}
.verificationCode{
  width:650rpx;
  height:40px;
  line-height:40px;
  margin:0 auto;
  border:1px solid #5e5e5e;
}

.my_head{
  height:200rpx;
  display:block;
  background:#000;
}
.my_head .head_Left{
  width: 150rpx;
  height:150rpx;
}
.my_head .head_Right{
  width: 100rpx;
  height:100rpx;
  margin-top: 50rpx;
}
.my_head .head_Left image{
  width: 150rpx;
  height:150rpx;
  border-radius:50%;
  margin-top:20rpx;
  margin-left:20rpx;
}
.my_head .head_Right image{
  width: 100rpx;
  height:100rpx;
}

.head_nav{
  border-bottom: 1px solid #999;
}
.head_nav_ul{
  display: block;
  height:50px;
  line-height:50px;
}
.head_nav_ul li{
  display: inline-block;
  width:187.5rpx;
  height:48px;
  line-height:50px;
  text-align:center;
}
.head_nav_ul .active{
  border-bottom: 2px solid #000;
}
/*********************************/
.unpaid_Remind{
  height:80rpx;
  line-height:80rpx;
  width: 750rpx;
  font-size:30rpx;
  border-bottom:1px solid #999;
}
.unpaid_Remind .fR{color:#ed9f9d;}
.c0{color:#000;}
.c3{color:#333;}

.all_List{font-size: 28rpx;}
.all_List .all_List_top{
  padding:20rpx 0;
  display: flex;
  flex-direction: row;
}
.all_List .img{
  width:200rpx;
  height:220rpx;
  margin-left: 20rpx;
}
.all_List .img image{
  width:200rpx;
  height:220rpx;
}
.all_List .text_Description{
  width:490rpx;
  margin-left:20rpx;
  position:relative;
  font-size:36rpx;
}
.all_List .price{
  width:180rpx;
  font-size: 32rpx;
  margin-right: 20rpx;
  text-align: right;
}
.all_List .price .goods_State{color: #ed9f9d;}
.all_List .goods_Numbwe{
  position: relative;
  top:-25rpx;
  right: 0;
}
.for_Goods .goods_Numbwe,.goods_Deal .goods_Numbwe{
  position: relative;
  top:60rpx;
  right: 0;
}
.all_List .total_Money{
  height:100rpx;
  line-height:100rpx;
  /* text-align: right; */
  /*color: #999;*/
  border-top:1px solid #c9c9c9;
  border-bottom:1px solid #c9c9c9;
}
.operation{
  display: flex;
  justify-content: flex-end;
  /* border-top:1px solid #ccc; */
  /* border-bottom:1px solid #ccc; */
  padding:15rpx 0;
  width:400rpx;
}
.operation .view_Information{
  width: 180rpx;
  height:68rpx;
  line-height:70rpx;
  border:1rpx solid #333;
  color: #333;
  background: #fff;
  text-align: center;
  margin-right:20rpx;
}
.operation .play{
  width: 150rpx;
  height:68rpx;
  line-height:70rpx;
  background:#424649;
  color: #fff;
  text-align: center;
  border:1rpx solid #424649;
  margin-right:20rpx;
}
/*********************************/
.no_Order{
  width: 500rpx;
  margin:30px auto;
  height:100px;
}
.no_Order image{
  width: 300rpx;
  height:200rpx;
}

.goods_Price{
  position: absolute;
  bottom: 0;
  width:100%;
}
.goods_Price .fL{
  font-size:30rpx;
  color:#ccc;
   position: absolute;
  bottom: 0;
}

.goods_Price .fR{
  font-size:36rpx;
}
.cC{color:#ccc;}
.f36R{font-size:36rpx;}

