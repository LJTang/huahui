          <!--<view class="layout_Left"><image src="/images/delete.png" mode='aspectFill'></image><span>押金</span></view>-->
          <!--<view class="layout_Right"><image src="/images/arrow-right.png" mode='aspectFill'></image></view>-->
        <!--</view>-->
      <!--</navigator>-->
      <navigator url="/pages/my_location/my_location" hover-class="none">
        <view class="layout_LR">
          <view class="layout_Left"><image src="/images/address.png" mode='aspectFill'></image><span>地址管理</span></view>
          <view class="layout_Right"><image src="/images/arrow-right.png" mode='aspectFill'></image></view>
        </view>
      </navigator>

      <view class="layout_LR">
        <view class="layout_Left"><image src="/images/kefu_phone.png" mode='aspectFill'></image><span>一键拨号</span></view>
        <view class="layout_Right"><image src="/images/arrow-right.png" mode='aspectFill'></image></view>
      </view>
    </view>

