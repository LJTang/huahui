.no_Order{
  width: 500rpx;
  margin:30px auto;
  height:100px;
}
.no_Order image{
  width: 300rpx;
  height:200rpx;
}
.cE4{color:#e4393c;}


/****************/
.address_List{
  border-bottom: 1px solid #ddd;
  align-items:center;
}
.address_List:last-child{
  border-bottom: 0;
}
.address_List radio{
  margin-right: 20rpx;
  transform:scale(0.7);
  margin-top: 20rpx;
}
.address_List .cen{
  width:650rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.address_List .cen .top text{
  font-size: 30rpx;
  padding-right: 20rpx;
}
.address_List .cen .down{
  font-size: 30rpx;
  color: #999;
  text-align: justify;
}
.address_List .right image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
  vertical-align:middle;
}
.bnt{
  position: fixed;
  bottom: 0;
  left: 0;
  border-radius: 0;
}

