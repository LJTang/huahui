  font-size:32rpx;
}
.footer .footer_Btn{
  height:100rpx;
  line-height:100rpx;
  /* border-top:1rpx solid #999; */
}
.footer .play{
  width: 200rpx;
  height:68rpx;
  line-height:70rpx;
  background:#424649;
